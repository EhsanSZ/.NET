<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <targets async="true">
    <target  name="logconsole" xsi:type="coloredconsole">
    </target>
  </targets>

  <rules>
    <!--<logger name="*" Levels="Trace,Debug,Fatal"  writeTo="logconsole"></logger>-->
    <logger name="*" minlevel="Info"  maxlevel="Warn" enabled="false" writeTo="logconsole"></logger>
  </rules>
</nlog>